#
# /etc/mk.conf: depls/depmk/mk configuration
#

# Repo order, the package found first is used
order punpun
order core
order 6c37-git
order 6c37
order opt
order xorg
order contrib

# Aliases
alias freetype=freetype-iu
alias fontconfig=fontconfig-iu
alias cairo=cairo-iu
alias gtk=gtk-ahoka
alias glib=glib-ahoka
alias libressl=openssl

# prtpatch dir
prtpatch /home/onodera/.prtpatch

# pkgmk command
makecommand pkgmk

# End of file
