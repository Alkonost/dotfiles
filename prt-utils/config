#
# /etc/mk.conf: depls/depmk/mk configuration
#

# Repo order, the package found first is used
order punpun
order core
order 6c37-git
order 6c37
order opt
order xorg
order contrib

# Aliases
alias opt/freetype 6c37/freetype-iu
alias opt/fontconfig 6c37/fontconfig-iu
alias opt/cairo 6c37/cairo-iu
alias opt/gtk 6c37/gtk-ahoka
alias opt/glib 6c37/glib-ahoka
alias core/openssl 6c37/libressl

# prtpatch dir
prtpatch /home/onodera/.prtpatch

# pkgmk command
makecommand pkgmk

# End of file
